{
  "name": "memory-snap-workspace",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "scripts": {
    "ci": "npx nx run-many --target=ci",
    "lint": "npx nx run-many --target=lint",
    "serve": "GENERATE_SOURCEMAP=false npx nx run-many --target=serve",
    "start": "npx nx run-many --target=start",
    "test": "npx nx run-many --target=test",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^8.0.0",
    "nx": "15.3.0"
  },
  "version": "1.0.1",
  "description": "Memory Snap monorepo",
  "repository": "git@github.com:perennialAutodidact/memory-snap.git",
  "author": "Keegan Good <perennialautodidact@gmail.com>",
  "license": "MIT"
}
